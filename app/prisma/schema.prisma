generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  telegramId   Int      @unique
  username     String?
  firstName    String?
  lastName     String?
  photoUrl     String?
  languageCode String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
